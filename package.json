{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "version": "0.11",
  "packageManager": "pnpm@10.10.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:fix": "eslint --ext .ts,.js,.vue . --fix",
    "format": "prettier --write \"**/*.{ts,js,vue,json,css,scss,md}\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts,js,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nuxt/eslint": "1.3.0",
    "@nuxt/fonts": "0.11.1",
    "@nuxt/icon": "1.12.0",
    "@nuxt/image": "1.10.0",
    "@nuxt/kit": "^3.16.2",
    "@nuxt/scripts": "0.11.6",
    "@nuxt/ui": "3.0.2",
    "@nuxthub/core": "0.8.24",
    "@nuxtjs/tailwindcss": "^6.13.2",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/vue-virtual": "^3.13.6",
    "@unhead/vue": "^2.0.3",
    "@vueuse/core": "^13.1.0",
    "@yeger/vue-masonry-wall": "^5.0.17",
    "eslint": "^9.0.0",
    "nuxt": "^3.16.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.6.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@nuxt/test-utils": "3.17.2",
    "@tailwindcss/postcss": "^4.1.4",
    "@testing-library/vue": "^8.1.0",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^17.4.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "playwright-core": "^1.52.0",
    "vitest": "^3.1.1",
    "wrangler": "^4.12.0"
  }
}
